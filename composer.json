{
    "name": "gdianov/opium",
    "type": "library",
    "description": "Simple dependency injection container",
    "keywords": ["di", "dependency injection", "container", "ioc", "psr-11", "psr11", "container-interop"],
    "license": "MIT",
    "require": {
        "symfony/yaml": "^5.1",
        "psr/container": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "gdianov\\opium\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "gdianov\\opium\\tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    }
}
